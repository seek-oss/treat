    "chalk": "^2.4.2",
    "css-loader": "^2.1.0",
    "css-selector-parser": "^1.3.0",
    "cssnano": "^4.1.10",
    "csstype": "^2.6.3",
    "dedent": "^0.7.0",
    "eval": "^0.1.2",
    "javascript-stringify": "^2.0.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "normalize-path": "^3.0.0",
    "object-hash": "^2.0.3",
    "postcss": "^7.0.14",

    "style-loader": "^0.23.1",
    "webpack-virtual-modules": "^0.1.10"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "bent": "^7.3.12",
